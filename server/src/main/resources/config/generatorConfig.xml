<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!--导入属性配置 -->
    <!--<properties resource="application-prod.properties"/>-->
    <properties resource="application.properties"/>
    <!--指定特定数据库的jdbc驱动jar包的位置 -->
    <!-- <classPathEntry location="${jdbc.driverLocation}"/>-->

    <context id="DB2Tables" targetRuntime="MyBatis3" defaultModelType="flat">
        <!-- 指定生成的java文件的编码,没有直接生成到项目时中文可能会乱码 -->
        <property name="javaFileEncoding" value="UTF-8"/>
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"></plugin>
        <!-- 这里的type里写的是你的实现类的类全路径 -->
        <commentGenerator type="com.admin.generator.MyCommentGenerator">
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/vue_admin?verifyServerCertificate=false&amp;useSSL=false&amp;requireSSL=false&amp;useUnicode=true"
                        userId="root"
                        password="">
            <!-- 针对oracle数据库 -->
            <property name="remarksReporting" value="true"></property>
        </jdbcConnection>

        <!--<jdbcConnection driverClass="${spring.datasource.driver-class-name}"-->
        <!--connectionURL="${var.dbUrl}" userId="${var.dbUserName}"-->
        <!--password="${var.dbPassword}">-->
        <!--</jdbcConnection>-->

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- dto class -->
        <javaModelGenerator targetPackage="com.admin.entity"
                            targetProject="../com_springboot_samples/11_springboot_generate/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- mybatis xml file -->
        <sqlMapGenerator targetPackage="com.admin.mapper"
                         targetProject="../com_springboot_samples/11_springboot_generate/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!-- mapper class -->
        <javaClientGenerator type="ANNOTATEDMAPPER"
                             targetPackage="com.admin.mapper"
                             implementationPackage="com.admin.mapper"
                             targetProject="../com_springboot_samples/11_springboot_generate/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--不生成帮助类（Exmaples） -->
        <!-- enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
            enableSelectByExample="false" selectByExampleQueryId="false" -->
        <!--已生成的表 <table schema="demo" tableName="USER" domainObjectName="User"></table> -->


        <table tableName="users" domainObjectName="User">
            <property name="useActualColumnNames" value="false"/>
            <generatedKey column="user_id" sqlStatement="JDBC" identity="true"/>
            <columnOverride column="requestContext" jdbcType="VARCHAR"/>
            <columnOverride column="responseContext" jdbcType="VARCHAR"/>
        </table>
        <table tableName="customers" domainObjectName="Customer">
            <property name="useActualColumnNames" value="false"/>
            <generatedKey column="customer_id" sqlStatement="JDBC" identity="true"/>
            <columnOverride column="requestContext" jdbcType="VARCHAR"/>
            <columnOverride column="responseContext" jdbcType="VARCHAR"/>
        </table>
        <table tableName="marketing" domainObjectName="Marketing">
            <property name="useActualColumnNames" value="false"/>
            <generatedKey column="marketing_id" sqlStatement="JDBC" identity="true"/>
            <columnOverride column="requestContext" jdbcType="VARCHAR"/>
            <columnOverride column="responseContext" jdbcType="VARCHAR"/>
        </table>
        <table tableName="loans" domainObjectName="Loan">
            <property name="useActualColumnNames" value="false"/>
            <generatedKey column="loan_id" sqlStatement="JDBC" identity="true"/>
            <columnOverride column="requestContext" jdbcType="VARCHAR"/>
            <columnOverride column="responseContext" jdbcType="VARCHAR"/>
        </table>
        <table tableName="repays" domainObjectName="Repay">
            <property name="useActualColumnNames" value="false"/>
            <generatedKey column="repay_id" sqlStatement="JDBC" identity="true"/>
            <columnOverride column="requestContext" jdbcType="VARCHAR"/>
            <columnOverride column="responseContext" jdbcType="VARCHAR"/>
        </table>
    </context>
